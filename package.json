{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/webpack --env.prod",
    "fresh": "rm -rf node_modules build reports",
    "lint:ci": "npm run lint -- --format junit --out reports/tslint.xml",
    "lint:fix": "npm run lint -- --fix",
    "lint": "./node_modules/.bin/tslint --project '.'",
    "test:ci": "npm run test -- --ci --reporters=default --reporters=jest-junit",
    "test:watch": "npm run test -- --watch",
    "test": "./node_modules/.bin/jest",
    "start": "./node_modules/.bin/webpack-dev-server --env.dev --watch"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "10.12.18",
    "@types/react": "16.7.21",
    "@types/react-dom": "16.0.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "jest": "^24.0.0",
    "jest-junit": "^6.1.0",
    "prettier": "^1.16.1",
    "raf": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/.*test.(ts|tsx|js)$",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ]
  }
}
