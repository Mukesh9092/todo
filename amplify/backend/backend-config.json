{
	"auth": {
		"todoAuth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation"
		}
	},
	"storage": {
		"todoStorage": {
			"service": "S3",
			"providerPlugin": "awscloudformation"
		}
	},
	"api": {
		"todoApi": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"securityType": "AMAZON_COGNITO_USER_POOLS"
			}
		}
	}
}